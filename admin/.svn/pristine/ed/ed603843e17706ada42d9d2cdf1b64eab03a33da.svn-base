.pagination>li>a,.pagination>li>span{font-size:14px;padding:8px 12px;}.pagination>li>a:hover,.pagination>li>span:hover,.pagination>li>a:focus,.pagination>li>span:focus{z-index:3;color:#ffffff;background-color:#bce8f1;border-color:transparent;box-shadow:none;}
.pagination>.active>a,.pagination>.active>span,.pagination>.active>a:hover,.pagination>.active>span:hover,.pagination>.active>a:focus,.pagination>.active>span:focus{z-index:2;color:#fff;background-color:#bce8f1;border-color:transparent;cursor:default;box-shadow:none;}
.pagination .go-page{position:relative;float:left;padding:0;margin-left:25px;line-height:1.42857143;color:#bce8f1;text-decoration:none;background-color:#fff;border:none;}.pagination .go-page .form-inline input{width:53px;height:38px;font-size:14px;text-align:center;}.pagination .go-page .form-inline input:focus{border-color:#bce8f1;box-shadow:none;transition-duration:0.5s;}
.pagination .go-page .form-inline .btn{position:relative;float:right;font-size:14px;padding:8px 12px;-webkit-border-radius:5px;-moz-border-radius:5px;border-radius:5px;margin-left:8px;line-height:1.42857143;color:#bce8f1;text-decoration:none;background-color:#fff;border:1px solid #dddddd;box-shadow:none;}
.pagination.pagination-mini .prev>a{margin-right:6px;border-top-left-radius:5px !important;border-bottom-left-radius:5px !important;}
.pagination.pagination-mini .next>a{margin-left:6px;border-top-right-radius:5px;border-bottom-right-radius:5px;}