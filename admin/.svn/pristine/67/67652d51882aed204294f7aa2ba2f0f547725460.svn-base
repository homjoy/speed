/* widget combobox */
.combobox .custom-combobox {
	position: relative;
	display: inline-block;
}
.combobox .custom-combobox-toggle {
	position: absolute;
	top: 0;
	bottom: 0;
	margin-left: -1px;
	padding: 0;
	z-index: 999;
}
.combobox .custom-combobox-input {
	margin: 0;
	color: #333;
	background: white;
	border-color: #ccc;
	border-radius: 0;
	font-weight: normal;
	font-size: 14px;
	line-height: 1.42857;
	padding: 6px 12px;
	text-align: center;
	vertical-align: middle;
	white-space: nowrap;
}
.ui-autocomplete {
	z-index: 9999;
	border-top-left-radius: 0;
	border-top-right-radius: 0;
}
.combobox .custom-combobox-toggle {
	background: white;
	border: 1px solid #ccc;
	line-height: 1.42857;
	padding: 6px 12px;
	cursor: pointer;
}
.combobox .custom-combobox-toggle:hover {
	text-decoration: none;
}
.combobox .tooltip {
	display: none !important;
}

.form-control{
	margin: 2px;
}