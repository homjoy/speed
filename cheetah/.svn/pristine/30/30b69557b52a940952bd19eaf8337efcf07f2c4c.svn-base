.mt-20{margin-top: -20px}
//下拉框
#statusSearch+.bootstrap-select{
  width: 160px !important;
  &>.btn.btn-default{
    background-color: white;
    border-color: white ;
  }
  &.open .btn.btn-default{
    border-color: #eeeeee !important;
  }
  .dropdown-menu{margin-top: 0}
}
.position-fix{
  position: absolute;
  background-color: white;
}
.nav-row{margin:37px 0}
.overfloarhidden{
  overflow: hidden;
  table:last-child{width: 120%}
}
.overfloarauto{overflow-y: auto;overflow-x: hidden}