@import "nav.less";
#page-content {
  padding: 20px 62px;
  min-width: 1030px;
}
#page-content{padding: 45px 52px}
.panel.gray{
  border: 1px solid #f4f4f4;
  border-bottom: none !important;
  .panel-heading,.panel-foot{
    height: 40px !important;
    &:after{border-bottom: 1px solid #f4f4f4}
    line-height: 40px;
  }
  &>.panel-heading,.panel-foot{
    background-color: #f4f4f4;
    &:after{border-bottom: hidden}
  }
  .panel{box-shadow:none}
}
.officesupply{
  border: solid 1px #fbfbfb;
  display: inline-block;
  width: 30%;
  margin: 0 1.3%;
  min-height: 85px;
  margin-bottom: 20px;
  background-color: #fbfbfb;
  padding: 5px;
  &.active{border: solid 1px #FEC1D6}
  &:hover{
    background-color: #f4f4f0;
    cursor: pointer;
  }
  .detail{
    display: block;
    height: 35px;
    overflow: auto;
    color: #a0a0a0;
  }
  h4{font-size: 14px}
}
.panel.left{width: 69%}
.panel.right{
  .pink.active{background-color: #F6FBFF}

  .panel-heading{padding-left: 10px}
  width: 30%;
  li{
    padding: 5px 15px 0;
  }
  .panel-foot{
    li{margin-bottom: 0}
  }
  .panel-body{
    padding: 0
  }

}
#navbar{
  z-index: 9999;
}
.supplytittle{
  display: inline-block;
  width: 60%;
  vertical-align: super;
}
.supplyconstrol{
  height: 23px;
  display: inline-block;
  width: 30%;
  position: relative;
  top: -5px;
  input{
    width: 30px;
    outline: none;
    cursor: default !important;
  }
  .minus,.plus{
    text-align: center;
    width: 20px;
    &:hover{border-color: pink;color: pink}
  }
  .numcontrol{
    float: left;
    border: solid 1px #f4f4f4;
    display: inline-block;
    height: 23px;
    line-height: 21px;
    min-width: 10px;
    max-width: 23px;
    cursor: pointer;
  }

}
.garbage{
  float: right;
  width: 15px;
  overflow: hidden;
  text-indent: 9999px;
  background:  url("../../../../img/speed/officesupply/delete.png") no-repeat;
  background-position: 0px 3px;
  position: relative;
  top: 7px;
  cursor: pointer;
}
.floornum{
  margin-left: 10px;
  width: 50% !important;
  margin-top: 7px;
  button.dropdown-toggle{line-height: 14px}
}
.dropdown.open>.btn, .btn-group.open .dropdown-toggle{
  box-shadow: none;
}
.approval-badge {
  color: orange;
  margin-left: 4px;
}
.submit{
  line-height: 18px !important;
  float: right;
  top: 7px;
  right: 10px;
  position: relative;
}
.menu-office-btn{
  height: 30px;
  width: 35px;
  float: left;
  display: inline;
  position: relative;
  margin-left: 50px;
  top: 10px;
  background:url("../../../../img/speed/officesupply/shaixuan.png") no-repeat;
  background-position: center;
  cursor: pointer;
  &.open{background-color: white}
}
.supplyname-div{
  width: 50%;
  position: relative;
  top: 3px;
  left: 20px;
  display: inline-block;
}
.supplyname{
  border-radius: 50px;
  position: relative;
  top: -4px;
}
.search-icon{
  background:white url("../../../../img/speed/officesupply/search.png") no-repeat;
  right: 7px;
  display: block;
  position: absolute;
  top: 3px;
  width: 20px;
  height: 20px;
  cursor: pointer;
}
.office-menu-detail{
  box-shadow: 2px 2px 2px rgba(0, 0, 0, 0.05);
  position: absolute;
  width: 100%;
  background: white;
  left: 0px;
  top: 40px;
  z-index: 9;
  a{
    display: inline-block;
    width:23%;
    height: 30px;
    margin-left: 1.3%;
    //margin-right: 1%;
    //background-color: blue;
  }
}
.pic{
  display: inline-block;
  width: 20px;
  height: 20px;
  position: relative;
  top: 6px;
  background-position: bottom;
  margin-right: 3px;
}
.picturnto{
  position: absolute;
  top: -130px;
  background-size: 100%;
}
.insidetittle{font-size: 14px}
.place{color: #FCBD4B}
.pic4{background: url("../../../../img/speed/officesupply/use_writing.png") no-repeat}
.pic5{background: url("../../../../img/speed/officesupply/use_desk.png") no-repeat}
.pic6{background: url("../../../../img/speed/officesupply/use_file.png") no-repeat}
.pic7{background: url("../../../../img/speed/officesupply/else.png") no-repeat}
.pic8{background: url("../../../../img/speed/officesupply/zhizhi.png") no-repeat}
.pic9{background: url("../../../../img/speed/officesupply/use_rdaily.png") no-repeat}
.pic10{background: url("../../../../img/speed/officesupply/use_finance.png") no-repeat}
.pic11{background: url("../../../../img/speed/officesupply/huiyi.png") no-repeat}
.pic12{background: url("../../../../img/speed/officesupply/zhoubian.png") no-repeat}
.pic3{background: url("../../../../img/speed/officesupply/use_massage.png") no-repeat}
.pic1{background: url("../../../../img/speed/officesupply/use_computerassert.png") no-repeat}
.pic2{background: url("../../../../img/speed/officesupply/use_usb.png") no-repeat}
.picbig{background-size: 100%}
.head-fix{
  width: 100%;
}
.fix{
  position: fixed;
  width: 624px;
  z-index: 5;
  top: 50px;
}
.fixright{
  position: fixed;
  width: 272px !important;
  top: 50px;
}
.over-scroll{
  min-height: 100px;
  max-height: 390px;
  overflow-y: scroll;
}
.kong{
  img{
    width: 100px;
    display: block;
    margin: 20px auto;
  }
  span{
    display: block;
    text-align: center;
    color: #DCDCDC;
    margin-bottom: 20px;
  }
}
.supplyinput{text-align: center}
.dropdown-menu li{
  &.selected a{
    background-color: #ff94b7;
    color: white;
  }
  padding-left: 0 !important;padding-right: 0 !important;
}
.dropdown-menu>li>a:hover{
  background-color: #f4f4f4 !important;
  color: black;
}
.supplytittle span{
  display: block;
  height: 17px;
}
.bootstrap-select.open>.btn.btn-default{
  border-color:#cdd6e1 ;
}
.right_detail{
  color: #a0a0a0;
}
.text-indent1000{
  text-indent: 1000px;
}
