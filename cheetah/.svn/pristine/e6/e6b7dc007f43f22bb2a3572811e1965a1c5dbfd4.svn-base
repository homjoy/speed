@import "../base.less";
// @import "../bootstrap.less";
*{
  margin:0;
  padding:0;
}
.form-control{display:inline;max-width:140px;}
.table-condensed{font-size: 14px;}
.viewBtn{
  color: #fff;
  background-color:#7ec228;
  border-color: #7ab500;
}
.viewBtn:hover, .viewBtn:focus, .viewBtn:active{
  color:#fff;
  background-color: #7cb900;
  border-color: #74a500;
}
.jumpday{
  background-color: #f5f5f5;
  background-image: -moz-linear-gradient(top, #ffffff, #e6e6e6);
  background-image: -webkit-gradient(linear, 0 0, 0 100%, from(#ffffff), to(#e6e6e6));
  background-image: -webkit-linear-gradient(top, #ffffff, #e6e6e6);
  background-image: -o-linear-gradient(top, #ffffff, #e6e6e6);
  background-image: linear-gradient(to bottom, #ffffff, #e6e6e6);
  background-repeat: repeat-x;
  border-color: #e6e6e6 #e6e6e6 #bfbfbf;
  border-color: rgba(0, 0, 0, 0.1) rgba(0, 0, 0, 0.1) rgba(0, 0, 0, 0.25);
  color: #333;
  text-shadow: 0 1px 1px rgba(255, 255, 255, 0.75);
  box-shadow: inset 0 1px 0 rgba(255, 255, 255, 0.2), 0 1px 2px rgba(0, 0, 0, 0.05);
}
.jumpday:hover, .jumpday:active{
  color: #333333;
  text-decoration: none;
  background-position: 0 -15px;
  -webkit-transition: background-position 0.1s linear;
     -moz-transition: background-position 0.1s linear;
       -o-transition: background-position 0.1s linear;
          transition: background-position 0.1s linear;
  background-color: #e6e6e6;
}
.jumpday_disabled{
  color: #333333;
  background-color: #e6e6e6;
  cursor: default;
  background-image: none;
  opacity: 0.65;
  box-shadow: none;
}
.globe_btn{
  width:45px;
  height:32px;
}
.table{
  table-layout: fixed;
}
ul{
  margin-bottom: 0;
}
.meeting_list li{
  list-style-type: none;
  float: left;
  text-align: left;
}
.time{
  word-wrap:break-word; 
  overflow:hidden;
  font-size: 12px;
}
.meeting_position{
  text-decoration: underline !important;
  font-size: 16px;
  display: inline !important;
  top:6px;
}
.bg{
  background: #ffb973;
}
.book_bg{
  background:#4088d1;
  color:#fff; 
}
.booked_bg{
  background:#ffb973; 
  color:#fff;
}
.time_0 .li_0{
  border-left: 1px solid transparent !important;
  border-top: 1px solid transparent !important;
  border-bottom: 1px solid transparent !important;
}
.time_13 .li_1{
  border-top: 1px solid transparent !important;
  border-bottom: 1px solid transparent !important;
}
#tab_head{
  margin-bottom: 0;
}
#tab_head thead>tr>th{
  border-bottom: 1px solid #ddd !important;
}
.col-width-1{
  width: 4.1880342%;
}
.col-width-2{
  width:8.3760684%;
}
.col-width-3{
  width:12.564103%;
}
.word_wrap{
  word-wrap: break-word;
}
.dropdown-menu li{
  float:none;
}
.dropdown-menu{
  min-width: 100px !important;
}
.d_position{
  display: block;
  cursor:pointer;
  text-align: center;
  font-weight: bold;
}
.d_position:hover{
  background: #e6e6e6;
}
.container-fluid{
  padding:0 15px;
  margin:0 auto;
}
.btn_group li{
  padding:10px 0;
  position:relative;
  display: block;
}
.my_right li{
  padding:9px 0;
}
.my_right .current{
  padding:6px 0 6px 14px;
}
.d_name{
  padding:6px 14px 6px 4px !important;
  cursor:pointer;
}
.meeting_position{
  font-size: 14px;
}

