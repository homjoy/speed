@import "./base.less";

#page-content {
  background: none;
  margin: 0;
  padding: 20px 25px 0;

  .panel{
    -webkit-border-radius: 5px;
    -moz-border-radius: 5px;
    border-radius: 5px;
  }

  #demo-dp-range .datepicker-days .prev,
  .panel_left .panel_head{
    border-radius: 5px 0 0 0;
  }
  .panel_right{
    .panel_head{
      border-radius: 5px 5px 0 0;
    }
  }
  .panel_time{
    .panel_right{
      .panel_head{
        border-radius: 0 5px 0 0;
      }
    }
  }
}

.left {
  float: left;
}

.right {
  float: right
}

.head_pic {
  margin-left: 4%;
  margin-top: -45px;
  height: 90px;
  width: 90px;
  top: 50%;
  position: relative;
  border-radius: 50px !important;
}

.head_word {
  position: relative;
  top: 50%;
  margin-top: -21px;
  margin-left: 6%
}

.head_word .up {
  color: #626262;
  font-size: 18px;
  display: block;
}

.head_word .down {
  color: #959595;
  font-size: 12px;
  display: block;
}

.select_div {
  width: 330px;
  top: 50%;
  position: relative;
  margin-top: -20px;
  left: 5%
}

.button_select {
  width: 70px;
  background-color: #ff6e9d;
  display: inline-block;
  cursor: default;
  height: 40px;
  font-size: 16px;
  color: white;
  text-align: center;
  padding-top: 8px;
  border-radius: 6px 0 0 6px
}

.button_select ul {
  background: #ff6e9d;
  padding: 0;
  position: relative;
  top: 7px;
  color: white
}

.button_select li {
  color: white;
  list-style: none;
  display: block;
  height: 40px;
  padding-top: 7px
}

.button_select:hover {
  color: white;
}

.input_select {
  height: 40px;
  border: 1px solid #ff6e9d;
  border-left: none;
  text-indent: 6px;
  box-shadow: none;
  width: 250px;
  border-radius: 0 6px 6px 0;
  outline: 0;
  &:focus{
    box-shadow: inset 0 1px 1px pink, 0 0 8px pink;
  }
}

.select_pic {
  height: 40px;
  width: 40px;
  display: block;
  position: absolute;
  right: 5px;
}

.select_pic img {
  width: 20px;
  margin-top: 9px;
}

.antenna ul {
  margin-top: 20px;
  padding-left: 8%
}

.antenna .panel_head {
  img {
    margin-right: 6px
  }
  color: #A0A0A0;
  font-size: 14px;
  padding-top: 18px;
  padding-left: 35px
}

.antenna li {
  list-style: none;
  display: inline-block;
  margin-right: 6%;
  margin-bottom: 20px;
  height: 67px
}

.antenna li img, .antenna li span {
  display: block;
  margin: 0 auto;
  text-align: center;
}

.antenna span {
  line-height: 1.2;
  padding-top: 10px;
}

.panel_right li {
  list-style: none;
  font-size: 14px;
  margin-bottom: 15px;
}

.panel_right li:first-child {
  color: green
}

.panel_right .panel_body li:first-child {
  color: green
}

.panel_right .panel_head {
  background-color: #707070;
  padding-top: 12px;
  height: 44px;
  color: white;
  text-align: center;
  font-size: 16px;
}

.panel_right .panel_body {
  .new {
    position: relative;
    top: -10px;
    left: -3px
  }
  padding: 20px 20px;
}

.panel_right .panel_body ul {
  padding: 0
}

.foot_panel .panel_head {
  font-size: 18px;
  padding-top: 12px;
  padding-left: 35px;
}

.foot_panel li {
  list-style: none;
  float: left;
  display: block;
  height: 130px;
  width: 50%
}

.foot_panel span {
  font-size: 18px;
  margin-top: 10px
}

.todo_today {
  height: 330px;
  margin-left: 316px;
  position: relative;
}

.todo_today .panel_body {
  background-color: #F4F4F4;
  height: 286px;

  div {
    max-height: 100%;
    overflow-y: auto;
  }
}

.todo_today ul {
  padding: 0
}

.todo_today .right {
  color: pink
}

.img_div {
  position: absolute;
  left: 50%;
  top: 50%;
  width: 150px;
  height: 135px;
  margin-left: -80px;
  margin-top: -68px;
  text-align: center;
}

.img_word {
  position: relative;
  top: -90px;
  right: -20px;
}

#demo-dp-range {
  float: left;
}

.datepicker-inline {
  border: none;
}

.table-condensed thead .color_change {
  background-color: #707070;
  height: 44px;
}

.foot_panel {
  height: 237px;
  width: 100%;
  background-color: #f2f5f7;
  color: #A0A0A0;
  min-width: 680px
}

.table-condensed {
  width: 316px
}

.mytime_rili {
  color: white;
  background-color: #F98FB3;
  font-size: 16px;
  width: 100px;
  height: 33px;
  display: block;
  text-align: center;
  line-height: 33px;
  margin: 0 auto;
  position: relative;
  top: -50px;
  border-radius: 5px;
}

.mytime_rili:hover {
  background-color: #FF3C7B;
  color: white
}

.more {
  display: block;
  text-align: center;
  color: #FF3C7B;
  font-size: 15px
}

#container {
  min-height: 100%;
  position: absolute;
  width: 100%
}

.something_to_do {
  border-radius: 30px !important;
  background-color: pink;
}

.share_time_div {
  height: 94px;
  min-width: 560px;
  overflow: hidden;
  .panel_body {
    background-color: #F98FB3;
    width: 94px;
    height: 100%;
    color: white;
    font-size: 14px;
    text-align: center;
    padding-top: 27px;
    float: left;
  }
  li {
    &:hover {
      background-color: #F3f3f3
    }
    &:focus {
      background-color: #E4E4E4
    }
    float: left;
    list-style: none;
    margin-left: 0px;
    margin-top: 8px;
    border-radius: 5px;
    & img {
      height: 60px;
      width: 60px;
      border-radius: 30px;
      margin-top: 5px;
      margin: 0 auto;
      display: block;
    }
    a {
      display: inline-block;
      width: 80px;
      height: 80px;
      span {
        display: block;
        text-align: center;
      }
    }
  }

}

.panel_time {
  min-width: 560px
}

.news_home {
  display: inline-block;
  max-width: 100%;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
  word-wrap: break-word;
}



.home-wizard{
  position: absolute;
  top:0;
  left:0;
  width: 100%;
  height:100%;
  display: block;
  z-index: 9999;
  background-color: rgba(0,0,0,0.65);
  .mask{

  }

  .wizard{
    opacity: 1;
    display:block;
    position: absolute;
    top:0;
    left:0;

    &.wizard-nav{
      position: fixed;
      width: 489px;
      height: 523px;
      top:50px;
      left:0;
      background: transparent url('../img/home/wizard/daohang.png') center 0 no-repeat;
    }

    &.wizard-often{
      width: 921px;
      height: 158px;
      background: transparent url('../img/home/wizard/changyong.png') center 0 no-repeat;
    }

    &.wizard-schedule{
      width: 647px;
      height: 338px;
      background: transparent url('../img/home/wizard/shijian.png') center 0 no-repeat;
    }

    &.wizard-user{
      position: fixed;
      top:0;
      right:0;
      width: 458px;
      height: 94px;
      background: transparent url('../img/home/wizard/gerenzhongxin.png') center 0 no-repeat;
    }

    &.wizard-close{
      position: fixed;
      right:5%;
      bottom:5%;
      width: 161px;
      height: 38px;
      background: transparent url('../img/home/wizard/guanbi.png') center 0 no-repeat;
    }
  }
}