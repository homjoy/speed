body {
  font-family: '微软雅黑', 'Microsoft YaHei' !important;
  letter-spacing: 1px;//字体间距.
}
#mainnav-container{
  width:180px;
}
@media (min-width:0px){
  #container.mainnav-lg #footer, #container.mainnav-lg #content-container{
    padding-left:180px;
  }
}
#container.mainnav-in.footer-fixed #footer,#container.mainnav-in #navbar,#container.mainnav-in #content-container,#container.mainnav-in #footer{
  left: 180px;
}



#mainnav-menu li a {
  position: relative;
}

.left_img {
  height: 16px;
  width: 20px;
  position: absolute;
  left: 14px
}

#mainnav-menu .menu-title {
  margin-left: 28px
}

.brand-icon-fyc {
  margin-left: 70px;
  float: left;
  margin-top: 3px
}

.down_load img {
  width: 110px;
}

.down_load a {
  text-align: center;
}

.head-list-user li img {
  margin-left: 20px;
  margin-right: 10px
}

.breadcrumb {
  margin-top: 15px;
  font-size: 14px
}


#mainnav-container {
  position: fixed !important;
}

header {
  position: fixed !important;
}

.pad-all {
  text-align: center;
}

.left {
  float: left;
}

.right {
  float: right;
}

.message_to_do {
  border-radius: 16px;
  padding: 0px 5px;
  color: white
}



.modal-backdrop {
  height: 100%;
  position: fixed;
}

// 面包屑
.breadcrumb {
  margin: 20px 0 !important;
}

.breadcrumb > .active > a {
  color: #777777
}

// 会议室插件
#calendar > table > tbody > tr > td.fc-header-left > span.fc-button.fc-button-prev.fc-state-default.fc-corner-left, #calendar > table > tbody > tr > td.fc-header-left > span.fc-button.fc-button-next.fc-state-default.fc-corner-right {
  height: 31px;
  line-height: 30px;
  &:hover {
    background-color: #F4F4F4;
  }
}

//右侧蓝色竖条bug
#navbar-container > div.navbar-content.clearfix > ul {
  max-height: 50px
}

// .fc-state-hover{background-color: #f4f4f4;color: black !important;}
// 行距重写
.panel {
  margin-bottom: 16px !important;
}

.modal-content {
  border-radius: 5px !important;
}

.btn {
  border-radius: 5px !important;
}

#navbar-container {
  max-height: 50px
}

#page-content {
  background: #fff;
  margin: 0px 25px;
  padding: 20px 30px;
  //各种圆角。。
  -webkit-border-radius: 5px;
  -moz-border-radius: 5px;
  border-radius: 5px;
}

#download-client {
  .head-list {
    > li:first-child {
      a:hover {
        background: #fff;
      }
    }
  }
}

#footer {
  .pad-lft {
    padding-left: 25px;
  }
  .pad-rgt {
    padding-right: 25px;
  }
}
//ul li 默认样式
ul li{list-style: none;}
ul{padding-left: 0;}
//自定义panel_pink样式
.panel_pink{
  height: 42px;
  background-color: #ff80A9;
  line-height: 42px;
  color: white;
  font-size: 16px;
  text-align: center;
  margin: 10px 40px;
}
//.btn样式
.btn-beauty{
  color: white;
  background-color: #F98FB3;
  font-size: 16px;
  line-height: 20px;
  display:inline-block;
  height: 33px;
  //text-align: center;
  //border-radius: 5px;
  &:hover{
    background-color: #FF3C7B;
    color: white;
  }
  &:focus{color:white;}
}
.has-success .form-control{
  border-color: #8abe2e;
}
.has-error .form-control{
  border-color: #f56245;
}
.first-title{
  color:#515151;}
.second-title{
  color: #626262;}
.thrid-title{
  color: #6a6a6a;}
.forth-title{
  color: #a0a0a0;}
@import "meeting/checkin";




.wizard-container{
  position: absolute;
  top:0;
  left:0;
  width: 100%;
  height:100%;
  display: block;
  z-index: 9999;
  background-color: rgba(0,0,0,0.65);

  .wizard{
    opacity: 1;
    display:block;
    position: absolute;

    &.wizard-check-in{
      position: fixed;
      right:5%;
      top:50px;
      width: 599px;
      height: 452px;
      background: transparent url('../img/wizard/checkin.png') center 0 no-repeat;
    }
    &.wizard-close-check-in{
      position: fixed;
      top: 260px;
      right:190px;
      width: 106px;
      height: 38px;
      background: transparent url('../img/wizard/close-checkin.png') center 0 no-repeat;
    }

    &.wizard-switch-overview{
      position: fixed;
      top: 113px;
      right: 39px;
      width: 281px;
      height: 253px;
      background: transparent url('../img/wizard/switch-overview.png') center 0 no-repeat;
    }
  }
}