.news_home {
  display: inline-block;
  max-width: 100%;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
  word-wrap: break-word;
  line-height: 1.0;
}

#demo-dp-range{
  position:absolute;
  top:0;
  left:25px;

  &:after{
    content: "-";
    display: block;
    overflow: hidden;
    width: 0px;
    height: 0px;
    border: 10px solid #fff;
    border-color: transparent transparent transparent #fff;
    border-width: 10px 0 10px 10px;
    position: absolute;
    top: 53%;
    z-index: 9;
    left: 318px;
  }


  .table-condensed {
    width: 294px;
  }
  .datepicker table tr th{
    border-radius: 0;
  }

  .datepicker-days table thead{
    //background: #eff2f6;
    background: transparent;
    color:#707070;
  }
  .datepicker-days table thead .dow_tr .dow{
    padding-top:25px;
    //content: "-";
    //display: block;
    //height: 5px;
    //overflow: hidden;
    //color: transparent;
  }

  .datepicker-days table tbody:before{
    content: "";
  }
  .datepicker-days table tbody tr td{
    padding: 0 5px 10px;
    background-color: transparent;
    border-color: transparent;
    i{
      display: block;
      width: 30px;
      height: 30px;
      border-radius: 30px;
      line-height: 30px;
      float: left;
      margin: 0;
      cursor: pointer;
      font-style: normal;
    }

    &.today{
      i{
        //background-color: #A9D5FF;
        //border-color: #A9D5FF;
        background-color: #FF649B;
        color:#fff;
      }
    }
    &.day:hover,&day.focused{
      background-position-y: 33px;
      font-weight: bold;
      i{
        //background: rgba(0, 0, 0, 0.1);
        background-color: #e6e6e6;
      }
    }
    &.day.active{
      i{
        //background-color: #489EED;
        //border-color: #489EED;
        background-color: #A0A0A0;
      }
    }
    &.day.active.something_to_do{
      i{
        background-color: #FCC4D4;
      }
    }
    &.something_to_do{
      background: transparent url("@{image-base}/home/dot_under_day.png") no-repeat center 26px;
      //i{
      //  background-color: pink;
      //}
      &.active,&.today{
        background-position-y: 33px;
      }
    }
  }
}
