@import "../../base.less";
@import "nav.less";
.position-r{
  position: relative;
}
#page-content{
  padding: 20px 62px;
}
.search-fyc{
  width: 185px !important;
}
.th-name{width: 130px}
//当前页样式
.right-info{
  .pad-lr78{
    padding: 25px 25px 0px 68px;
    position: relative;
  }
  .pad-lr70{
    padding: 25px 25px 0px 60px;
    position: relative;
  }
}
.show-info{
  height: 54px;
}
dl{
  width: 100%;
  display: block;
  text-align: left;
  dt{
    text-align: right;
    float: left;
    width: 14%;
  }
  dd{
    padding-left: 5%;
    float: left;
    width: 86%;
    margin-bottom: 8px;
    .tracking_id{margin: 0}
    .weight{margin-bottom: 8px}
  }
}
.avatar-name,.depart-date{
  display: inline-block;
  text-align: center;
  float: left;
}
.depart-date{
  margin-bottom: 40px;
  height: 47px;
  width: 80%;
  dl{
    .bottom{
      position: relative;
    }
    dt{width:30%}
    dd{
      width: 70%;
      margin-bottom: 5px
    }
  }
}
.timelinepl,.timeline{
  padding-left: 3px;
  &:before{
    margin-left: 3px;
  }
}


.badge-warning {
  background-color: #ebaa4b !important;
}
.left-dt{
  max-width: 75px
}
.left-pink{
  border-left:solid 3px #ff84aa ;
}

.hide-td-narrow{
  .checkth{width:10%}
  .nameth{width: 40%}
}
.hide-td-fat{
  .checkth{width:5%}
  .nameth{width: 20%}
  .dateth{width: 15%}
}
//右下footer样式
.info-foot{
  position: absolute;
  bottom: 0px;
  height: 40px;
  background-color: #f6f8fa;
  width: 100%;
  left: 0;
  line-height: 40px;
  padding:0 20px;

}
.speed-search-head .usernameSearch-div {
  min-width: 180px ;
}
.speed-search-head {
  padding-left: 0px !important;
}
#page-content{min-width: 1035px;}
.approval-history{margin-left:5px}
.remark{margin-top:3px}
.dropdown-menu>li>a:hover {
  background-color: #ff80a9;
  color: #ffffff;
}
hr{
  float: left;
  width: 90%;
  margin-top: 8px;
  margin-bottom: 16px;
  border-style: dashed;
}

#statusSearch+.bootstrap-select{
  &>.btn span{
    color: #ff6397;
  }

}




