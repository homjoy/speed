.right{float: right;}
.message_to_do{  border-radius: 16px;padding: 0px 5px;color: white}
// 我的时间
.myself_time,.share_time_head{
  list-style: none;text-align:center;float:left;margin-bottom:10px;padding-top:12px;width:100px;height:100px;color: black;font-size: 14px;
  margin-left: 5px;border-radius: 5px;
  &:hover{background-color: #f2f2f2;border-radius: 5px;}
  &:active{background-color: #E3E3E3;border-radius: 5px;}
}
.modal-backdrop{height: 100%;position: fixed;}
// 面包屑
.breadcrumb{margin: 20px 0 !important;}
// 会议室插件
#calendar > table > tbody > tr > td.fc-header-left > span.fc-button.fc-button-prev.fc-state-default.fc-corner-left,#calendar > table > tbody > tr > td.fc-header-left > span.fc-button.fc-button-next.fc-state-default.fc-corner-right{
  height: 31px;line-height: 30px;
  &:hover{background-color: #F4F4F4;}
}
#calendar > table > tbody > tr > td.fc-header-left > span.fc-button.fc-button-today.fc-state-default.fc-corner-left.fc-corner-right.fc-state-disabled{
  background-color: #5fa2dd;color: white;
  opacity: 1;
  &:hover{background-color: darken(#5fa2dd,5%) !important;}
}
