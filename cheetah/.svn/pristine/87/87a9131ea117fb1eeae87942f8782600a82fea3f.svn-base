//新闻版块样式
.newsDiv{
  position: relative;
}
#newsCarousel,
.newsList {
  float: left;
}
.newsList{
  position: absolute;
  width:~"calc(100% - 365px)";
  padding-left: 5px;
  padding-right: 5px;
  left: 360px;
  z-index:10;

  ul{
    max-height: 186px;
    overflow: hidden;
    margin-bottom: 0;
  }

  li{
    margin-bottom: 7px;

    a:hover{
      color: #ff6397 !important;
    }

    &:nth-of-type(1){
      padding-top: 10px;

      a{
        max-width: 86%;

        overflow: hidden;
        white-space: nowrap;
        text-overflow: ellipsis;
      }

    }

    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
  }
}
#newsCarousel {
  width:330px;
  height: 186px;

  .carousel-indicators{
    height: 30px;
    top: 15px;
    left: 90%;
    z-index: 1;

    li{
      width:6px;
      height: 6px;
      //background-color: dodgerblue;
      margin-right: 10px;
      margin-bottom: 5px;
    }
  }

  .carousel-control{
    color:#fff;
    padding-top:20px;
  }

  .fa{
    &:before {
      background-color: rgba(0, 0, 0, 0.1);
    }

    &:hover{
      background-color: rgba(0, 0, 0, 0.2);
    }
  }

  .item{
    a{
      display: inline-block;
    }

    img{
      height: 186px;
      width: 330px;
    }
  }
  .carousel-caption{
    width: 330px;
    line-height: 28px;
    left: 0;
    bottom: 5px;
    padding: 0;
    background-color: rgba(0,0,0,0.6);

    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
  }

}
.more {
  display: block;
  margin-top:10px;
  color: #f58fb5;

  &:hover{
    color:#ff6397;
  }
}

//快捷键
.shortcut-content{
  padding: 16px 20px 10px 20px;
  text-align: center;

  ul{
     list-style: none;
     margin-bottom: 0;

     li{
       display: inline-block;
       margin-right: 3%;
     }

     a{
       display: inline-block;
       line-height: 0;
       position: relative;
       width:80px;
       height: 80px;

       div{
         position: relative;
         top: 0;
         width: 80px;
         height: 45px;
         margin: 0 auto;
         overflow: hidden;
         background-size:contain;
         background-position: 50%  110% !important;
         -webkit-transition: all 0.1s ease-in-out;
         -moz-transition: all 0.1s ease-in-out;
         -o-transition: all 0.1s ease-in-out;
         transition: all 0.1s ease-in-out;
       }
       p{
         position: absolute;
         bottom: 10px;
         width:80px;
         text-align: center;
       }

       &:hover div{
         background-position:50% 0 !important;
         -webkit-transition: all 0.1s ease-in-out;
         -moz-transition: all 0.1s ease-in-out;
         -o-transition: all 0.1s ease-in-out;
         transition: all 0.1s ease-in-out;
       }

     };

   }

  .meeting-apply{
    background:transparent url("../../../img/speed/newIcon/shortcut/meeting.png") no-repeat scroll 0 0;
  }
  .leave-apply{
    background:transparent url("../../../img/speed/newIcon/shortcut/leave_apply.png") no-repeat scroll 0 0;

  }
  .officesupply-apply{
    background:transparent url("../../../img/speed/newIcon/shortcut/office_apply.png") no-repeat scroll 0 0;

  }
  .asset-apply{
    background:transparent url("../../../img/speed/newIcon/shortcut/asset_apply.png") no-repeat scroll 0 0;

  }
  .travel-apply{
    background:transparent url("../../../img/speed/newIcon/shortcut/Business_apply.png") no-repeat scroll 0 0;

  }
  .userExpMoney-apply{
    background:transparent url("../../../img/speed/newIcon/shortcut/ux.png") no-repeat scroll 0 0;

  }
}

