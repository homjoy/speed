.button_select {
  width: 70px;
  background-color: #ff6e9d;
  display: inline-block;
  cursor: default;
  height: 40px;
  font-size: 16px;
  color: white;
  text-align: center;
  padding-top: 8px;
  border-radius: 6px 0 0 6px
}

.button_select ul {
  background: #ff6e9d;
  padding: 0;
  position: relative;
  top: 7px;
  color: white
}

.button_select li {
  color: white;
  list-style: none;
  display: block;
  height: 40px;
  padding-top: 7px
