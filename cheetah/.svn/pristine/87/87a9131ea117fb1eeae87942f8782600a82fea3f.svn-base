//日历样式
.datepicker-inline {
  border: none;
}

.table-condensed thead .color_change {
  background-color: #f7f8fa;
  color:#727272;
  height: 44px;

  .datepicker-switch{
    width:auto;
    font-size:15px;
    &:hover{
      background-color: #f7f8fa;
    }
  }

  .prev,.next{
    display: inline-block;
    position: absolute;
    top: 10px;
    padding: 0;
    width: 25px;
    height: 25px;
    border-radius: 12px !important;

    font-weight: bolder;
    font-size: 16px;
    color: #fff;
    background-color: #DDDEDD;
  }

  .prev{
    left: 5%;
  }
  .next{
    right:5%;
  }
}

#demo-dp-range{
  position: absolute;
  top: 0;
  left: 6%;

  &:after{
    content: "-";
    display: block;
    overflow: hidden;
    width: 0px;
    height: 0px;
    border: 10px solid #fff;
    border-color: transparent transparent transparent #fff;
    border-width: 10px 0 10px 10px;
    position: absolute;
    top: 53%;
    z-index: 9;
    left: 318px;
  }


  .table-condensed {
    table-layout: fixed;
    width: 262px;
  }
  .datepicker table tr th{
    border-radius: 0;
  }

  .datepicker-days table thead{
    background: transparent;
    color: #C5CCCF;
  }
  .datepicker-days table thead .dow_tr .dow{
    padding-top:25px;
    //content: "-";
    //display: block;
    //height: 5px;
    //overflow: hidden;
    //color: transparent;
  }

  .datepicker-days table tbody:before{
    content: "";
  }
  .datepicker-days table tbody tr td{
    padding: 0 6px 14px 6px;
    background-color: transparent;
    border-color: transparent;
    i{
      display: block;
      width: 25px;
      height: 25px;
      border-radius: 30px;
      line-height: 25px;
      float: left;
      margin: 0;
      cursor: pointer;
      font-style: normal;
    }

    &.today{
      i{
        //background-color: #A9D5FF;
        //border-color: #A9D5FF;
        background-color: #FF649B;
        color:#fff;
      }
    }
    &.day:hover,&day.focused{
      background-position-y: 28px;
      font-weight: bold;
      i{
        //background: rgba(0, 0, 0, 0.1);
        background-color: #e6e6e6;
      }
    }
    &.day.active{
      i{
        //background-color: #489EED;
        //border-color: #489EED;
        background-color: #A0A0A0;
      }
    }
    &.day.active.something_to_do{
      i{
        background-color: #FCC4D4;
      }
    }
    &.something_to_do{
      background: transparent url("@{image-base}/home/dot_under_day.png") no-repeat center 24px;
      //i{
      //  background-color: pink;
      //}
      &.active,&.today{
        background-position-y: 28px;
      }
    }
  }
}
