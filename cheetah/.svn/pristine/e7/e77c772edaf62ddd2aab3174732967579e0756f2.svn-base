//统一的消息提示框.
.queen-msg-container {
  width: 100%;
  height: 48px;
  position: fixed;
  top: 50px;
  right: 0;
  z-index: 3000;
  display: none;
  text-align: center;
  .message {
    margin: 0 auto;
    width: 370px;
    //max-width: 500px;
    line-height: 48px;
    height: 48px;
    color: #fff;
    font-size: 14px;
    overflow: hidden;
    text-overflow: ellipsis;
    word-break: break-all;
    box-shadow: 0 2px 8px rgba(0, 0, 0, 0.21);

    &.success {
      background-color: #A5CE78;
    }
    &.info {
      background-color: #A5CE78;
    }
    &.warning {
      background-color: #f4bf4f;
    }
    &.error {
      background-color: #F47B5B;
    }
  }
}


