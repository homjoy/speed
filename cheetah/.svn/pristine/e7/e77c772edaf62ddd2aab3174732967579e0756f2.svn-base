// 公共样式————————按钮
.btn {
  -webkit-border-radius: 5px;
  -moz-border-radius: 5px;
  border-radius: 5px;
  font-size: 14px;
  border: none;
  color: white;
  &:hover {
    color: white;
  }
  &:active {
    color: white;
  }
  &:focus {
    color: white;
  }
}

//--大小分类
.btn-big {
  height: 32px;
  font-size: 16px;
}

.btn-small {
  min-width: 68px;
  height: 28px;
  font-size: 14px;
}

.btn-sup-small {
  min-width: 72px;
  height: 22px;
  font-size: 12px;
}

// --颜色分类
.btn-beauty {
  background-color: #f98fb3;
  &:hover {
    background-color: #ff377a;
  }
  &:active {
    background-color: #f98fb3;
  }
}

.btn-primary, .btn-edit {
  background-color: #5b9be0;
  &:hover {
    background-color: #3b8dd5;
  }
  &:active {
    background-color: #5b9be0;
  }
}

.btn-default {
  color: #515151;
  &:hover {
    color: #515151;
    background-color: #e9e9e9;
    border: #cdd5e2;
  }
  &:active {
    color: #5b9be0;
  }
  &:focus {
    color: #515151;
  }
}

.btn-own {
  width: 60px;
  height: 30px;
}

.btn-agree {
  background-color: #68c664;
  &:hover {
    background-color: #7eb242;
  }
  &:active {
    background-color: #68c664;
  }
}

.btn-reject, .btn-del {
  background-color: #f2663e;
  &:hover {
    background-color: #e14e2b;
  }
  &:active {
    background-color: #f2663e;
  }
}

.btn-hurry {
  background-color: #eab53f;
  &:hover {
    background-color: #cd9b2d;
  }
  &:active {
    background-color: #eab53f;
  }
}

