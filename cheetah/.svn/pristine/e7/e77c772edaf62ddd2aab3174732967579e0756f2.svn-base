body {
  font: 12px/1.125 Arial, Helvetica, sans-serif;
  letter-spacing: 1px;
  font-family: "Helvetica Neue", Helvetica, Microsoft Yahei, Heiti, "黑体", Hiragino Sans GB, WenQuanYi Micro Hei, sans-serif;
}

ul {
  list-style: none;
  padding: 0;
}

.table > tbody > tr > td,
.table > tbody > tr > th,
.table > tfoot > tr > td,
.table > tfoot > tr > th,
