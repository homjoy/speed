// 我的时间
.panel{box-shadow: none}
#message-container{
  width:100%; top:50px; z-index: 100; position: fixed;display:none;
  .container{width:400px;}
}
#calendar{margin-top:15px}
#message-container{
  .container{width:400px;}
  width:100%; top:70px; z-index: 100; position: fixed;display:none;
}
div.panel.panel-default > div{background: white;
  border:none ;}
blockquote{
  display: block;
  margin:0;
  padding: 18px 0 0 0;
  font-size:12px;
  border: none !important;
  a{
    display: inline-block;
    margin-bottom: 7px;
    img{margin-right: 5px;}
    font-size:14px;
    color: #ff6e9d;
    &:first-child{
      margin-right: 70px;
    }
    &:hover{color: #ff6e9d;}
  }
  abbr{
    border-bottom: 1px dotted #999;
    font-size:12px;
    margin-top: 3px;
    color:#6a6a6a;
  }
}
// 头像
.myself_time, .share_time_head {
  list-style: none;
  text-align: center;
  //float: left;
  display: inline-block;
  margin-bottom: 10px;
  padding-top: 12px;
  width: 100px;
  height: 100px;
  //color: black;
  color:#6a6a6a;
  font-size: 14px;
  margin-left: 5px;
  border-radius: 5px;
  img{
    border:1px solid #dddddd;
    //padding: 1px 0 ;
    display:block;
    border-radius:30px;
    margin:0 auto;
  }
