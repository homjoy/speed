// 高亮

.share_high_light {
  span{color:rgba(252,140,178,1); }
  img{
    border:2px solid rgba(252,140,178,0.5);
  }
}
// 悬停
  #tip {
      position: absolute;
      width: 250px;
      max-width: 400px;
      text-align: left;
      padding: 4px;
      border: 0;
      /*border-radius: 6px;  */
      z-index: 1000;
      background-color: white;
      border: 1px solid #999999;
      -webkit-box-shadow: 0 5px 15px rgba(0, 0, 0, 0.5);
      box-shadow: 0 5px 15px rgba(0, 0, 0, 0.5);
  }

  #tip ul {
      margin: 0;
      padding: 0;
      li{
        font-family: 'Microsoft YaHei', '微软雅黑', 'Microsoft JhengHei', '宋体';
        font-size: 14px;
        list-style: none !important;
        padding-left: 10px;
      }
  }

  
  @media (min-width: 550px) and (max-width: 700px) {
      .fc-header-title h2 {
          font-size: 20px
      }
  }

  @media (min-width: 500px) and (max-width: 549px) {
      .fc-header-title h2 {
          font-size: 16px
      }
  }

  @media (max-width: 499px) {
      .fc-header-title h2 {
          display: none;
      }
  }

  #container {
      min-height: 100%;
      position: absolute;
      width: 100%
  }