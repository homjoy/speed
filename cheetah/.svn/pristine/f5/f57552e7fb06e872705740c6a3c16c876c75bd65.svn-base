#navbar{
  width:100%;
}

#navbar-container {
  min-width:1000px;
  height: 80px;
  line-height: 80px;
  background-color: #FD8DB3;

  //设置图标
  .navbar-header-fyc{
    width:12%;
    height: 80px;
    float:left;
    margin-left: 20px;
    margin-right: 70px;
  }

  //设置搜索框
  .navbar-contact-search{
    float: left;
    position: relative;
    //margin-right:20%;

    > input{
      line-height: 30px;
      height: 34px;
      border-left: none;
      text-indent: 6px;
      box-shadow: none;
      width: 300px;
      border: 1px solid #fff;
      border-radius: 6px;
      outline: 0;

      &:focus{
        box-shadow: 0 0 0 1px #eeeeee;
      }
    }

    .search_pic{
      display: block;
      line-height: 0;
      position: absolute;
      top: 30px;
      right: 10px;
      opacity: 0.5;
      img{
        width:20px;
      }
    }

  }

  //个人信息
  .navbar-user-info{
    float: right;
    position: relative;
    width:400px;

    a{
      display: inline-block;
      line-height: 0;
    }

    p{
      line-height: normal;
      font-size:smaller;
      transform:scale(0.92);
      margin-bottom:2px;
      opacity: 0.8;
    }

    > a {
      position: absolute;
      top:0;
      left: 0;
      display: inline-block;
      width: 20%;
      height: 80px;
      padding: 10px;

      img{
        width:60px;
        height: 60px;
        border-radius: 30px;

        &:hover{
          box-shadow: 0 0 0 2px #fd4583;
        }
      }
    }

    .user-info-describe{
      position: absolute;
      top:0;
      left:20%;
      padding-top:13px;
      color:#fff;
      fon-size:10px;

      .describe-detail{
        padding-left: 10px;
        padding-bottom: 5px;
        line-height: 32px;
        font-size: 17px;

        .messageinfo{
          margin: 0 15px 10px 15px;

          > div{
            top: 26px;
            right: -20px;

            ul{
              padding-top: 10px;
            }

            li{
              padding-top: 5px;
              text-align: center;

              &:hover{
                background-color: #eee;
              }
            }

            a{
              display: inline-block;
              line-height: 15px;

              span:nth-of-type(1){
                margin-right: 40px;
              }
            }
          }

        }
      }

      .entry-days{
          font-size:16px;
          padding-left: 5px;
          padding-right: 5px;

          color:#fff000;
      }

      .badge{
        width: 16px;
        min-height: 16px;
        border-radius: 8px;
        font-size: 10px;
        padding-left: 4px;
        padding-top: 3px;
        background-color: #fd4583;
        top: 53%;
        right:-8px;
      }

    }

    .related-info{
      position: absolute;
      right: 10%;
      top:10px;
      width: 70px;
      height: 60px;
      line-height: 34px;
      font-size: 10px;
      transform: scale(0.93);

      > span {

        > a {
          color:#fff;
          opacity: 0.8;

          &:hover{
            color:#eee;
          }
        }

        .dropdown-menu{
          top:150%;
        }

        ul{
          line-height: 40px;
          margin-bottom: 0;

          li:hover{
            background-color: #eeeeee;
          }
        }
      }

      >span:nth-of-type(1){
        > div{
          right: 61%;
        }
        a{
          line-height: 20px;
          width: 160px;
        }
      }
      //第一个span－客户端下载
      >span:nth-of-type(2){
        margin-right: 50px;
        width:200px;

        ul{
          text-align: center;

          li{
            padding:5px 0 ;
          }
          li:nth-of-type(1){
            &:hover{
              background-color: #fff;
            }
          }
          width: 198px;
          margin-top: 20px;
          margin-bottom: 10px;
        }
      }
    }
  }
  //去掉右侧竖线
  .navbar-content {
    box-shadow: 0 0 0 0  #fff;
  }
}

