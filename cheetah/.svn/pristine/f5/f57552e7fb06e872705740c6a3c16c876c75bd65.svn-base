//label样式
.speed label{
  line-height: 30px;
  font-size: 14px;
}
// 输入框样式
.speed input:not([type="radio"]):not([type="checkbox"]){
  height: 42px !important;font-size: 14px !important;
}
//下拉框。
.select-div{
  height: 44px;
  z-index: 9;
  .select-input{
    cursor: pointer !important;
    background-color: white;
    background: url(../../img/speed/xiala.png) no-repeat 280px 15px;
  }

}
.select-ul{
  box-shadow:0px 2px 8px rgba(0, 0, 0, 0.21);
  z-index: 9;
  border: 1px solid #eee;
  border-top: none;
  width:100%;
  cursor: pointer;
  li{
    text-indent: 10px;
    line-height: 42px;
    font-size: 14px;
    background: white;
    display: block;
    height: 42px;
    width: 100%;
    &:hover{background-color: #eee;}
  }
}
