

//下面提示
.safe-notice{
  
  margin: 0 auto;
  width: 600px;
  p{
    margin-bottom: 0;
  }
  span{
    color: #5b9be0;
    margin-left: 10px;
  }
  .vpn_notice > a{
    color: #5b9be0;
    margin-left: 10px;
    .glyphicon{
      color: #000;
    }
  }
  .notice{
    padding:1px 10px 10px 90px;
    margin-bottom: 80px;
    background-color: #f6f8fa;
    color:#6a6a6a;
    p{//公用部分，待提取
      font-size: 12px !important;
    }
  }
  .indent {
    display: block;
    margin-top:10px;
    color:#f39700;

    .btn-danger{
      background-color: #98c664;
      border-color:  #98c664;
      color:#fff;
      margin-left:5px;
    }
  }
}
//侧面提示
.password_notice {
  color: #6a6a6a;
  span {
    font-size: 12px !important;
    color: #ff80a9
  }
  h4{
    font-size:14px;
  }
  p {
    font-size: 12px;
  }
  .indent {
    display: block;
    margin-top:10px;
    color:#f39700;

    .btn-danger{
      background-color: #98c664;
