<style type="text/css">

    body {
        -webkit-text-size-adjust: 100% !important;
        margin: 0 !important;
        padding: 0 !important;
        font-family: "Microsoft YaHei", "微软雅黑" !important;
    }
    ul{
        list-style:none;
        padding:0;
        margin:0;
    }
    ul > li{
        margin-bottom: 12px;
    }

    @media only screen and (max-device-width: 768px) {

    }

    .mail-wrapper,.logo {
        width: 480px;
        margin: 0 auto;
    }
    .mail-wrapper{
        padding-bottom: 60px;
    }
    .logo {
        text-align: center;
        padding: 36px 0 33px 0;
    }
    .logo a{
        display: block;
        text-indent: -9999px;
        width: 66px;
        height: 76px;
        margin: 0 auto;
        background: url("http://static.newspeed.meilishuo.com/img/mail/newspeed_logo.png");
    }

    .title{
        padding: 0 0 28px 0;
    }
    .title p,h3{
        margin:0;
        font-size: 20px;
        font-weight: normal;
    }
    .title p{
        color:#F991B4;
        margin-bottom: 14px;
    }
    .title h3{
        color:#707070;
    }

    .section{
        padding:18px 0 10px 0;
    }
    .section.service{
        padding-bottom: 0;
        margin-bottom: -6px;
    }
    .section .place{
        margin-bottom: 26px;
    }
    .section .place:last-child{
        margin-bottom: 0;
    }

    span.tt{
        color:#b2b2b2;
        margin-right: 32px;
        font-weight: 400;
        vertical-align: top;
    }
    span.ct{
        color:#666;
        display: inline-block;
        width:360px;
    }
    .line {
        border-bottom: 1px solid #eee;
    }

    .btn-group{
        text-align: center;
        padding:50px 0;
    }
    .btn {
        display: inline-block;
        width: 88px;
        height: 41px;
        line-height: 41px;
        text-align:center;
        color:#707070;
        text-decoration: none;
        border: 1px solid #eee;
        border-radius: 8px;
        /*background: url("data:image/png;base64,") no-repeat 0 0;*/
    }
    .btn-accept{
        margin-right: 40px;
    }

    .icon{
        margin:0 5px -4px 0;
        display: inline-block;
        width: 19px;
        height: 19px;
    }
    .icon-accept {
        background: url("data:image/png;base64,") no-repeat 0 0;
    }
    .icon-decline {
        background: url("data:image/png;base64,") no-repeat 0 0;
    }

    .footer {
        text-align: center;
    }
    .footer .tips{
        color:#7c7c7c;
    }
    .footer .go-speed{
        color:#a2a2a2;
    }
    .link-my-time {
        color: #b3d463;
        text-decoration: underline;
    }

    .important{
        color:#eb1e3f !important;
    }
    .st{
        color:#666;
        display: inline-block;
        width:360px;
        line-height: 30px;
        margin:0;
        font-size: 14px;
    }

</style>
<div class="logo">
    <a href="http://speed.meilishuo.com" target="_blank">美丽说SPEED</a>
</div>

